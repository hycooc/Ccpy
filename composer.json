{
    "name": "hycooc/ccpy",
    "description": "laravel ccpy",
    "type": "library",
    "license": "MIT",
    "keywords": ["pinyin", "ccpy"],
    "authors": [
        {
            "name": "baoerge",
            "email": "baoerge123@163.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "graham-campbell/manager": "^2.5",
        "illuminate/contracts": "^5.4",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Hycooc\\Ccpy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hycooc\\Tests\\Ccpy\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hycooc\\Ccpy\\CcpyServiceProvider"
            ],
            "aliases": {
                "Ccpy": "Hycooc\\Ccpy\\Facades\\Ccpy"
            }
        }
    }
}